{
  "name": "application-frame",
  "version": "0.7.0-dev.0",
  "description": "Application Frame is a small JavaScript Framework / Library at a realy early stage.",
  "main": "af.js",
  "dependencies": {
    "callsite": "^1.0.0"
  },
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "chai": "^4.0.1",
    "coveralls": "^2.11.15",
    "eslint": "^4.1.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-clean": "^0.3.2",
    "gulp-sourcemaps": "^2.4.0",
    "ink-docstrap": "^1.2.1",
    "istanbul": "^0.4.5",
    "merge-stream": "^1.0.0",
    "mocha": "^3.2.0",
    "remap-istanbul": "^0.9.0",
    "eslint-plugin-scanjs-rules": "^0.2.1"
  },
  "scripts": {
    "build": "gulp --gulpfile .travis/Gulpfile.js --cwd ./",
    "test": "node_modules/.bin/mocha .travis/main.js",
    "coverage": "istanbul cover _mocha -- .travis/main.js --report json",
    "remap-core": "remap-istanbul -i coverage/coverage.json -o coverage/mapped.json -b ./",
    "remap-cov": "npm run coverage && npm run remap-core && istanbul report --include \"coverage/mapped.json\" lcov",
    "coveralls": "npm run remap-cov && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TitanNanoDE/ApplicationFrame.git"
  },
  "keywords": [
    "framework",
    "library",
    "application-frame"
  ],
  "author": "TitanNano.de",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/TitanNanoDE/ApplicationFrame/issues"
  },
  "homepage": "https://github.com/TitanNanoDE/ApplicationFrame#readme"
}
